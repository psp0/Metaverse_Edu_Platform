.animated-label {
    margin-top: 18%;
    width: 61%;
    white-space: normal;
    color: rgb(60, 72, 71);
    -unity-font-definition: url('project://database/Assets/_ThirdParty/Fonts/Roboto/Roboto-Medium%20SDF.asset?fileID=11400000&guid=db5974fdd47364381a7edc91a9df9b2f&type=2#Roboto-Medium SDF');
