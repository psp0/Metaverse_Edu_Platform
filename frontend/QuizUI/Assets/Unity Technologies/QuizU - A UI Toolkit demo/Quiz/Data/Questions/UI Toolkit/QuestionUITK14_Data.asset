  m_QuestionText:
  - FontSize: 2
    Text: In UI Toolkit, what is the purpose of <b>BindableElement</b>?
  m_QuestionImage: {fileID: 0}
  m_Skills: UI Toolkit
  m_ShuffleAnswers: 1
  m_Answers:
  - IsCorrect: 1
    Text: It allows elements to be bound to data sources
  - IsCorrect: 0
    Text: It allows elements to be bound to event handlers
  - IsCorrect: 0
    Text: It allows elements to be bound to animations
